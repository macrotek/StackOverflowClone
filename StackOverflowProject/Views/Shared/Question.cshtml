@model StackOverflowProject.ViewModels.QuestionViewModel
<tr>
    <td><span class="badge">@Model.VotesCount</span> Votes</td>
    <td><span class="badge">@Model.AnswersCount</span>Answers</td>
    <td><span class="badge">@Model.ViewsCount</span>Views</td>
    <td class="text-dark">
        <a href="/questions/view/@Model.QuestionName" class="text-primary">@Model.QuestionName</a>
        <br />
        <span class="badge badge-info">@Model.Category.CategoryName</span>
    </td>
    <td>
        @if (Convert.ToInt32(Session["CurrentUserID"]) != item.UserID)
        {
            @:by @Model.User.Name
        }
        else
        {
            @:by you
        }
    </td>
    <td> On @Model.QuestionDateAndTime</td>
</tr>