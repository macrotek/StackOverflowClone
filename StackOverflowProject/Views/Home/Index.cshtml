@model List<StackOverflowProject.ViewModels.QuestionViewModel>
@{ 
    ViewBag.Title="Home";
}
<div class="row">
    <div class="col-sm-12">
        <div class="jumbotron">
            <h1>Learn, Share And Build Yourself..!</h1>
            <h3>Practice makes man perfect - Just add atleast one github commit on a day.</h3>
            <p class="lead">Join the world's largest developer community here.</p>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-sm-12">
        <table class="table">
            @foreach(var item in Model)
            {
                Html.RenderPartial("Question", item);
            }
        </table>
        @if(Session["CurrentUserName"] != null)
        {
            <a href="/questions/create" class="btn btn-primary"> Add Question</a>
        }
    </div>
</div>
<script>
    $("a[href='/home/index']").attr("id", "active");
</script>